../../../src/cxrt \
     -prefix sde \
     -nx 5 \
     -nr 50 \
     -nt 200 \
     -velocity 50.0 \
     -diffusioncoefficient 1.0e-6 \
     -DfD0 1.0 \
     -DeD0sphere 1.0 \
     -inletconcentration 1.0 \
     -initialconcentration 1.0 \
     -simulationduration 200.0 \
     -kp 0.0 \
     -noutput 20

# vary nr from 10, 20, to 50 to examine the impact of temporal discretizatin
# kp = 0 to simulate no sorption
# initial and inlet concentration = 1 to simulate diffusion into spheres only
# DfD0, DeD0sphere to make film and sphere diffusion coefficient to be the same as molecular diffusion coefficient 
